<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<ThingDef ParentName="BuildingBase">
		<defName>FD_CMM_CheeseShelf</defName>
		<label>cheese shelf</label>
		<description>A shelf for aging curds into cheese, works best in a room with temperature between 12째C-15째C (53째F-59째F).</description>
<!--		<thingClass>Building_FermentingBarrel</thingClass> -->
		<drawerType>MapMeshAndRealTime</drawerType>
		<graphicData>
			<texPath>Things/Building/FD_CMM_CheeseShelf/FD_CMM_CheeseShelf</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<drawSize>(3,2)</drawSize>
		</graphicData>
		<defaultPlacingRot>South</defaultPlacingRot>
		<castEdgeShadows>true</castEdgeShadows>
		<minifiedDef>MinifiedThing</minifiedDef>
		<altitudeLayer>Building</altitudeLayer>
		<passability>PassThroughOnly</passability>
		<fillPercent>0.4</fillPercent>
		<pathCost>50</pathCost>
		<thingCategories>
			<li>BuildingsProduction</li>
		</thingCategories>
		<statBases>
			<WorkToBuild>500</WorkToBuild>
			<Mass>8</Mass>
			<MaxHitPoints>100</MaxHitPoints>
			<Flammability>1.0</Flammability>
		</statBases>
		<size>(2,1)</size>
		<costStuffCount>20</costStuffCount>
		<stuffCategories>
			<li>Metallic</li>
			<li>Woody</li>
			<li>Stony</li>
		</stuffCategories>
		<comps>
			<li Class="ProcessorFramework.CompProperties_Processor">
				<capacity>20</capacity>
				<independentProcesses>false</independentProcesses>
				<parallelProcesses>false</parallelProcesses>
				<dropIngredients>true</dropIngredients>
				<showProductIcon>true</showProductIcon>
				<barOffset>(0.0, 0.25)</barOffset>
				<barScale>(1.0, 1.0)</barScale>
				<colorCoded>false</colorCoded>
				<processes>
					<li>FD_Ricotta</li>
					<li>FD_Mozzarella</li>
					<li>FD_BlueCheese</li>
					<li>FD_Camembert</li>
					<li>FD_Feta</li>
					<li>FD_Parmesan</li>
					<li>FD_Gouda</li>
					<li>FD_Cheddar</li>
				</processes>
			</li>
		</comps>
		<tickerType>Rare</tickerType>
		<rotatable>true</rotatable>
		<designationCategory>Production</designationCategory>
		<uiOrder>2710</uiOrder>
		<building>
			<buildingTags>
				<li>Production</li>
			</buildingTags>
			<destroySound>BuildingDestroyed_Wood_Small</destroySound>
		</building>
		<researchPrerequisites>
			<li>FD_CM_BasicCheesemaking</li>
		</researchPrerequisites>
	</ThingDef>
	
	<ThingDef ParentName="BuildingBase">
		<defName>FD_CMM_CheeseFridge</defName>
		<label>cheese fridge</label>
		<description>An electrical refrigerator calibrated to the perfect temperature for aging curds into cheese, with twice the capacity of a shelf.</description>
<!--		<thingClass>Building_FermentingBarrel</thingClass> -->
		<drawerType>MapMeshAndRealTime</drawerType>
		<graphicData>
			<texPath>Things/Building/FD_CMM_CheeseFridge/FD_CMM_CheeseFridge</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<drawSize>(2,1)</drawSize>
			<damageData>
				<cornerTL>Damage/Corner</cornerTL>
				<cornerTR>Damage/Corner</cornerTR>
				<cornerBL>Damage/Corner</cornerBL>
				<cornerBR>Damage/Corner</cornerBR>
			</damageData>
		</graphicData>
		<defaultPlacingRot>South</defaultPlacingRot>
		<castEdgeShadows>true</castEdgeShadows>
		<minifiedDef>MinifiedThing</minifiedDef>
		<altitudeLayer>Building</altitudeLayer>
		<passability>PassThroughOnly</passability>
		<fillPercent>0.4</fillPercent>
		<pathCost>42</pathCost>
		<thingCategories>
			<li>BuildingsProduction</li>
		</thingCategories>
		<statBases>
			<WorkToBuild>2000</WorkToBuild>
			<Mass>15</Mass>
			<MaxHitPoints>120</MaxHitPoints>
			<Flammability>1.0</Flammability>
		</statBases>
		<size>(2,1)</size>
		<costList>
			<Steel>50</Steel> <!-- Change this -->
			<ComponentIndustrial>1</ComponentIndustrial>
		</costList>
		<comps>
			<li Class="CompProperties_Power">
				<compClass>CompPowerTrader</compClass>
				<shortCircuitInRain>true</shortCircuitInRain>
				<basePowerConsumption>50</basePowerConsumption>
			</li>
			<li Class="CompProperties_Flickable"/>
			<li Class="CompProperties_Breakdownable"/>
			<li Class="ProcessorFramework.CompProperties_Processor">
				<capacity>40</capacity>
				<independentProcesses>false</independentProcesses>
				<parallelProcesses>false</parallelProcesses>
				<dropIngredients>true</dropIngredients>
				<showProductIcon>true</showProductIcon>
				<barOffset>(0.0, 0.25)</barOffset>
				<barScale>(1.0, 1.0)</barScale>
				<colorCoded>false</colorCoded>
				<processes>
					<li>FD_Ricotta_Fridge</li>
					<li>FD_Mozzarella_Fridge</li>
					<li>FD_BlueCheese_Fridge</li>
					<li>FD_Camembert_Fridge</li>
					<li>FD_Feta_Fridge</li>
					<li>FD_Parmesan_Fridge</li>
					<li>FD_Gouda_Fridge</li>
					<li>FD_Cheddar_Fridge</li>
				</processes>
			</li>
		</comps>
		<tickerType>Rare</tickerType>
		<rotatable>true</rotatable>
		<designationCategory>Production</designationCategory>
		<staticSunShadowHeight>0.5</staticSunShadowHeight>
		<canOverlapZones>false</canOverlapZones>
		<uiOrder>2710</uiOrder>
		<building>
			<buildingTags>
				<li>Production</li>
			</buildingTags>
		</building>
		<researchPrerequisites>
			<li>FD_CM_StandardCheesemaking</li>
			<li>Electricity</li>
		</researchPrerequisites>
	</ThingDef>
	
	<ThingDef ParentName="BuildingBase">
		<defName>FD_CMM_CheeseVat</defName>
		<label>cheese vat</label>
		<description>An electrically-powered vat for producing large volumes of cheese curds automatically.</description>
<!--		<thingClass>Building_FermentingBarrel</thingClass> -->
		<drawerType>MapMeshAndRealTime</drawerType>
		<graphicData>
			<texPath>Things/Building/FD_CMM_CheeseVat/FD_CMM_CheeseVat</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<drawSize>(2.2,2.2)</drawSize>
			<shadowData>
				<volume>(2, 1, 1.7)</volume> <!-- Figure this out -->
			</shadowData>
		</graphicData>
		<defaultPlacingRot>South</defaultPlacingRot>
		<altitudeLayer>Building</altitudeLayer>
		<passability>PassThroughOnly</passability>
		<fillPercent>0.9</fillPercent>
		<pathCost>50</pathCost>
		<thingCategories>
		</thingCategories>
		<statBases>
			<WorkToBuild>2500</WorkToBuild>
			<MaxHitPoints>300</MaxHitPoints>
			<Flammability>1.0</Flammability>
		</statBases>
		<size>(2,2)</size>
		<costList>
			<Steel>75</Steel>
			<ComponentIndustrial>1</ComponentIndustrial>
		</costList>
		<comps>
			<li Class="CompProperties_Power">
				<compClass>CompPowerTrader</compClass>
				<shortCircuitInRain>true</shortCircuitInRain>
				<basePowerConsumption>75</basePowerConsumption>
			</li>
			<li Class="CompProperties_Flickable"/>
			<li Class="CompProperties_Breakdownable"/>
			<li Class="ProcessorFramework.CompProperties_Processor">
				<capacity>40</capacity>
				<independentProcesses>false</independentProcesses>
				<parallelProcesses>false</parallelProcesses>
				<dropIngredients>true</dropIngredients>
				<showProductIcon>true</showProductIcon>
				<barOffset>(0.0, 0.25)</barOffset>
				<barScale>(1.0, 1.0)</barScale>
				<colorCoded>false</colorCoded>
				<processes>
					<li>FD_Curd</li>
				</processes>
			</li>			
		</comps>
		<tickerType>Rare</tickerType>
		<rotatable>true</rotatable>
		<designationCategory>Production</designationCategory>
		<uiOrder>2710</uiOrder>
		<building>
			<destroySound>BuildingDestroyed_Metal_Medium</destroySound>
			<buildingTags>
				<li>Production</li>
			</buildingTags>
		</building>
		<researchPrerequisites>
			<li>FD_CM_AdvancedCheesemaking</li>
			<li>Electricity</li>
		</researchPrerequisites>
	</ThingDef>

</Defs>
