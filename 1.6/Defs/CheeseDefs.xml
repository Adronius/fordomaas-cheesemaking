<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<ThingDef ParentName="MealBase">
		<defName>FD_CMM_Camembert</defName>
		<label>camembert</label>
		<description>A moist, soft, creamy, surface-ripened cow's milk cheese.</description>
		<graphicData>
			<texPath>Things/Cheeses/Camembert</texPath>
			<graphicClass>Graphic_StackCount</graphicClass>
		</graphicData>
		<tradeability>Buyable</tradeability>
		<stackLimit>10</stackLimit>
		<socialPropernessMatters>true</socialPropernessMatters>
		<statBases>
			<MaxHitPoints>60</MaxHitPoints>
			<MarketValue>15</MarketValue>
			<Mass>0.025</Mass>
			<Flammability>1.0</Flammability>
			<DeteriorationRate>8</DeteriorationRate>
			<Nutrition>0.5</Nutrition>
		</statBases>
		<thingCategories>
			<li>Cheeses</li>
		</thingCategories>
		<rotatable>false</rotatable>
		<ingestible>
			<preferability>RawTasty</preferability>		
			<joy>0.07</joy>
			<joyKind>Gluttonous</joyKind>
			<maxNumToIngestAtOnce>2</maxNumToIngestAtOnce>
			<defaultNumToIngestAtOnce>1</defaultNumToIngestAtOnce>
			<ingestEffect>EatVegetarian</ingestEffect>
			<ingestSound>Meal_Eat</ingestSound>
		</ingestible>
	</ThingDef>
	
